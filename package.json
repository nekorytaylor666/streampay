{
  "name": "streamflow-app",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "tsc --noEmit && eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json,md}": "prettier --write",
    "**/*.ts{,x}": [
      "eslint --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.1",
    "@hookform/resolvers": "^2.8.3",
    "@project-serum/anchor": "^0.16.2",
    "@project-serum/sol-wallet-adapter": "^0.2.4",
    "@solana/spl-token": "^0.1.6",
    "@solana/spl-token-registry": "^0.2.265",
    "@solana/wallet-adapter-base": "^0.5.1",
    "@solana/wallet-adapter-wallets": "^0.7.4",
    "@solana/web3.js": "^1.20.0",
    "@streamflow/timelock": "^0.4.1-beta.10",
    "@tailwindcss/forms": "^0.3.2",
    "@types/yup": "^0.29.13",
    "buffer-layout": "^1.2.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.4",
    "sweetalert": "^2.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11",
    "zustand": "^3.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^9.8.6",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "^7.0.36",
    "prettier": "^2.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "typescript": "^4.4.3"
  }
}
